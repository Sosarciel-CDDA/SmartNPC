{
    require_mod: "magiclysm",
    table: {
        //轻微痛感
        //当使用某些法术时造成少量疼痛。
        biomantic_pain_caused_light: null,

        //血肉缝合
        //使目标的伤口蠕动，血肉自行聚合，从而治愈其伤势。
        //此术虽诡异可怖，痛苦难当，却颇有奇效。
        light_healing: {
            cast_condition: [{
                hook: "BattleUpdate",
                condition: { or: [
                    { math: ["n_hp('torso')", "<=", "n_hp_max('torso')/3"] },
                    { math: ["n_hp('head')", "<=", "n_hp_max('head')/3"] }
                ]}
            },{
                hook: "SlowUpdate",
                condition: { or: [
                    { math: ["N_SumMaxHp()-N_SumHp()", ">", "10"] },
                    { math: ["n_hp('torso')", "<=", "n_hp_max('torso')/3"] },
                    { math: ["n_hp('head')", "<=", "n_hp_max('head')/3"] }
                ]}
            }],
            weight: -1
        },

        //伤痛分流
        //平摊各个肢体受到的伤害。
        pain_split: {
            cast_condition: [{
                hook: "BattleUpdate",
                condition: { and: [
                    { math: ["U_MaxHp()-U_MinHp()", ">", "max(U_AvgHp()/2, 10)"] },
                    { or: [
                        { math: ["u_hp('torso')", "<=", "u_hp_max('torso')/3"] },
                        { math: ["u_hp('head')", "<=", "u_hp_max('head')/3"] }
                    ]}
                ]}
            },{
                hook: "SlowUpdate",
                condition: { and: [
                    { math: ["U_MaxHp()-U_MinHp()", ">", "max(U_AvgHp()/2, 10)"] },
                    { or: [
                        { math: ["u_hp('torso')", "<=", "u_hp_max('torso')/3"] },
                        { math: ["u_hp('head')", "<=", "u_hp_max('head')/3"] }
                    ]}
                ]}
            }]
        },

        //残暴之鞭
        //此法术会从你的体内伸出一条又长又恶心的鞭状触手，其上布满尖骨，流淌着酸液，具有长距离攻击能力和凶猛的伤害。
        vicious_tentacle: {
            type: "BattleSelfBuffCond",
            condition: { not: { u_has_item: "tentacle_whip" } }
        },

        //进化版残暴之鞭
        //此法术会从你的体内伸出一条又长又恶心的鞭状触手，其上布满尖骨，流淌着酸液，具有长距离攻击能力和凶猛的伤害。
        //现在你对此法术已了如指掌，并开始着手设计属于你自己的版本。
        vicious_tentacle_plus: {
            type: "BattleSelfBuffCond",
            condition: { not: { u_has_item: "tentacle_whip" } }
        },

        //扭曲强化
        //此法术以诡异、非自然的方式扭曲你的身体，以增强你的体能与力量。
        bio_grotesque: "BattleSelfBuff",

        //酸液喷射
        //当施法时，法师张开他的嘴，向前方扇形范围内的敌人喷射腐蚀性酸液，将它们溶解成黏液。
        bio_acidicspray: "TargetDamage",

        //肉身成袋
        //这个法术会在你的背部皮肤长出一个大袋子，可以让你存储物品。
        bio_fleshpouch: null,

        //白骨之矛
        //这个法术可以召唤出一只邪恶的骨矛，拥有长长的手柄及尖锐的刀刃。
        bio_bonespear: {
            type: "BattleSelfBuffCond",
            condition: { not: { u_has_item: "bonespear" } }
        },

        //进化版白骨之矛
        //此法术会创造一根长长的骨矛，矛尖锋利，矛身遍布利刃。
        //现在你对此法术已了如指掌，并开始着手设计属于你自己的版本。
        bio_bonespear_plus: {
            type: "BattleSelfBuffCond",
            condition: { not: { u_has_item: "bonespear" } }
        },

        //符文（生化术士）
        //此法术会创造一根长长的骨矛，矛尖锋利，矛身遍布利刃。
        //现在你对此法术已了如指掌，并开始着手设计属于你自己的版本。
        create_rune_biomancer: null,

        //麻痹毒刺
        //喷射出一根由筋腱与骨头纠缠而成的扭曲细针，它造成的螫刺会迟缓受害者。
        biomancer_paralytic_dart: "TargetDamage",

        //脏腑四射
        //在你四周喷射出辛辣刺鼻的血肉，不断生长并将你的猎物捕获进一片有毒的卷须之中。
        biomancer_visceral_projection: "SelfAoeDamage",

        //凝结组织
        //将你对生化魔法的掌握转化至体内，增强你的肉体。
        //与医疗能量不同，它通过防止失血，在伤口化脓之前消除伤口，代价是加速新陈代谢，容易饥饿及口渴。
        biomancer_coagulant_weave: "BattleSelfBuff",

        //腐蚀光环
        //这个法术在你身体周围召唤出一个酸液光环，腐蚀攻击你的敌人，并增加你的近战伤害。
        biomancer_caustic_aura: "BattleSelfBuff",

        //强化腐蚀光环
        //此仪式会让你身上长出一个小肉瘤，它会迅速脱落，化为一枚同调了生化术士的符文。
        //你可以将此符文用作配方的催化剂。
        biomancer_caustic_aura_plus: "BattleSelfBuff",

        //蛙肢
        //手脚上长出蹼，让快速游泳变得轻而易举。
        biomancer_swim_frog: null,

        //巨型生长
        //成长为巨大的体型。你的衣服和装备不会随你成长。
        biomancer_giant_growth: null,

        //稳定基因
        //此法术会在你周身悬浮一层酸液，在受到攻击时腐蚀近战攻击者，并增强你的近战伤害。
        //现在你对此法术已了如指掌，并开始着手设计属于你自己的版本。
        biomancer_remove_instability: null,

        //腐肉盛宴
        //改变你的消化系统，让你吃下腐烂的食物而无任何不良反应。
        //这要么是一个真正古老的咒语，要么一定是某所大学法医学研究部门的产物。
        biomancer_carrion_feast: null,

        //鞭笞触手
        //卷轴上说，此法术用于“减轻XE037暴露所带来的负面影响”，不管那是什么意思。
        //根据卷轴所述，应待法术效果完全结束后方可再次施放。
        biomancer_lashing_tentacles: null,

        //涤病术
        //从你的双肩长出一对挥舞的触手。虽然不及“凶猛触手”那般长，但它们会自动攻击任何与你近战的敌人。
        //长出触手后，你将无法穿戴覆盖肩部的硬质盔甲。
        biomancer_cure_disease_minor: null,

        //涤病术效果
        //此条目触发涤病术的实际治疗效果，因为呕吐类法术无法应用 effect_str。
        biomancer_cure_disease_minor_actual_effect: null,

        //肉体再生术
        //将你可能患上的任何轻微疾病，如感冒或流感，一并呕出。
        //这是为数不多能对他人施放的生化系治疗法术之一。
        biomancer_hyper_regeneration: null,

        //止血
        //随着你能控制自己的身体，你也能控制自己的血液。
        biomancer_slow_bleeding: null,

        //指刃术
        //将你的每根手指都变成无数微小的骨刃，让你能以手术般的精度进行切割……或是将敌人切成碎片。
        biomancer_scalpel_fingers: null,

        //酸血术
        //将你的血液转化为被魔法强化的酸液，使你免疫酸液，同时会向使你流血的生物喷溅酸液。
        biomancer_caustic_blood: null,

        //暴饮暴食
        //扩容你的胃，使它能够装下更多食物，不会挤占其他内脏的空间。
        //曾经是个无甚卵用的咒语，现在它终于有用了，因为你不知道下一顿饭什么时候才能吃。
        biomancer_eat_tons_of_food: null,

        //猩红灌注
        //以水为催化剂，恢复你部分流失的血液。
        //如果你健康无伤，此法术将毫无效果。
        biomancer_get_more_blood: null,

        //塑形奥义
        //卷轴上说，此法术用于“对形态发生之变动施加审慎的控制”，听起来有些不祥。
        biomancer_control_mutations: null
    }
}
